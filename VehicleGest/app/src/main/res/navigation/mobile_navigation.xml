        tools:layout="@layout/fragment_vehicles" />

    <fragment
        android:id="@+id/navigation_services"
        android:name="es.ilerna.proyectodam.vehiclegest.ui.services.ServicesFragment"
        android:label="@string/title_vehicles"
        tools:layout="@layout/fragment_vehicles" />

    <fragment
        android:id="@+id/navigation_employees"
        android:name="es.ilerna.proyectodam.vehiclegest.ui.employees.EmployeesFragment"
