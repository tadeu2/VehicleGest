        tools:layout="@layout/fragment_vehicles" />
    <fragment
        android:id="@+id/navigation_inventory"
        android:name="es.ilerna.proyectodam.vehiclegest.ui.inventory.InventoryFragment"
        android:label="@string/title_vehicles"
        tools:layout="@layout/fragment_vehicles" />

    <fragment
        android:id="@+id/navigation_alerts"
        android:name="es.ilerna.proyectodam.vehiclegest.ui.alerts.AlertsFragment"
        android:label="@string/title_alerts"
        tools:layout="@layout/fragment_alerts" />
    <activity
        android:id="@+id/mainActivity"
        android:name="es.ilerna.proyectodam.vehiclegest.ui.MainActivity"
        android:label="MainActivity" />


</navigation>